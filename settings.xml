<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      https://maven.apache.org/xsd/settings-1.0.0.xsd">
	<servers>
		<server>
			<id>github</id>
			<username>mrbraztech</username>
			<password>${env.GITHUB_REGISTRY_TOKEN}</password>
		</server>
	</servers>

	<profiles>
		<profile>
			<id>default</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<repositories>
				<repository>
					<id>archmix</id>
					<url>https://maven.pkg.github.com/archmix</url>
				</repository>
			</repositories>
		</profile>
	</profiles>
</settings>